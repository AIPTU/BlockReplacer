{
    "name": "aiptu/blockreplacer",
    "description": "A PocketMine-MP plugin that replaces a block with another block at a predetermined time.",
    "version": "3.0.0-dev",
    "license": "MIT",
    "type": "project",
    "require": {
        "pocketmine/pocketmine-mp": "5.0.0-BETA2"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
