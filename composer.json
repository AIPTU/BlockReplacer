{
    "name": "aiptu/blockreplacer",
    "description": "A PocketMine-MP plugin that replaces a block with another block at a predetermined time.",
    "license": "MIT",
    "type": "project",
    "require": {
        "pocketmine/pocketmine-mp": "^5.0",
        "aiptu/sounds": "^1.3",
        "latampmdevs/update-notifier": "dev-master"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "AIPTU\\BlockReplacer\\": "/src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/LatamPMDevs/UpdateNotifier.git"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
